  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "npx prisma generate --schema database/mysql/prisma/schema.prisma",
    "prisma:migrate": "npx prisma migrate dev --schema database/mysql/prisma/schema.prisma",
    "prisma:push": "npx prisma db push --schema database/mysql/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema database/mysql/prisma/schema.prisma",
    "db:deploy": "npx prisma migrate deploy --schema database/mysql/prisma/schema.prisma && yarn run prisma:generate",
    "db:local": "sh ./database/init-user.sh"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.828.0",
